#calculations
print(34+43)
print(34-43)
print(34*43)
print(34/43)
print(34//43)
#variables
a=24
b=34
print(a+b)
print(a-b)
print(a*b)
print(a/b)
print(a//b)
#strings
print("Raghunandhana")
print("raghuvarasevana")
print("shivakarthikeyan")
#types
hello_var=33.484
print(type(hello_var))
#length
str="hi buddy"
print(len(str))
#indexing
print(str[1])
print(str[3])
print(str[-3])
print(str[-7])
print(str[2:6])#slicing
print(str[3:4])
